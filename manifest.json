{
  "manifest_version": 3,
  "name": "Printify Transactions Count",
  "description": "Get printify transactions totals",
  "version": "1.0",
  "action": {
    "default_popup": "popup/main.html",
    "default_icon": "images/printify.png"
  },
  "host_permissions": [
    "https://printify.com/*"
  ],
  "permissions": [
    "activeTab",
    "scripting"
  ]
}